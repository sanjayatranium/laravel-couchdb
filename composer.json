{
  "name": "rbewley4/laravel-couchdb",
  "description": "CouchDB database driver for Laravel 4",
  "keywords": ["laravel", "couchdb", "couch", "database"],
  "authors": [
    {
      "name": "Russell Bewley",
      "homepage": "https://github.com/rbewley4"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=5.3.0",
